        "# Step2. Skewness and Kurtosis\n",
        "\n",
        "# Skewness and Kurtosis scores\n",
        "skewness = numeric_features.skew().round(3)\n",
        "kurtosis = numeric_features.kurt().round(3)\n",
        "\n",
        "# Display skewness and kurtosis scores\n",
        "print(\"\\nSkewness:\")\n",
        "print(skewness)\n",
        "print(\"\\nKurtosis:\")\n",
        "print(kurtosis)"
